{
  "name": "quizjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "~16.4.2",
    "react-dom": "~16.4.2",
    "react-hot-loader": "~4.3.4",
    "tslib": "~1.9.3"
  },
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "stylelint": "stylelint ./src/*.css ./src/***/*.css",
    "stylelint-fix": "stylelint ./src/*.css ./src/***/*.css --fix",
    "tslint": "tslint --project . --force",
    "tslint-fix": "tslint --project . --fix",
    "lint": "npm run tslint && npm run stylelint",
    "lint-fix": "npm run tslint-fix && npm run stylelint-fix",
    "profile": "webpack --profile --mode=production --json > profiling.json",
    "bundle-report": "npm run profile && webpack-bundle-analyzer profiling.json"
  },
  "devDependencies": {
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "autoprefixer": "~9.1.0",
    "clean-webpack-plugin": "~0.1.19",
    "copy-webpack-plugin": "~4.5.2",
    "css-loader": "~1.0.0",
    "file-loader": "~1.1.11",
    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.4.1",
    "normalize.css": "~8.0.0",
    "offline-plugin": "~5.0.5",
    "optimize-css-assets-webpack-plugin": "~5.0.0",
    "postcss-loader": "~3.0.0",
    "source-map-loader": "~0.2.3",
    "style-loader": "~0.22.1",
    "stylelint": "~9.4.0",
    "stylelint-order": "~1.0.0",
    "svg-url-loader": "~2.3.2",
    "ts-loader": "~4.4.2",
    "tslint": "~5.11.0",
    "typescript": "~3.0.1",
    "uglifyjs-webpack-plugin": "~1.2.7",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "~2.13.1",
    "webpack-cli": "~3.1.0",
    "webpack-dev-server": "~3.1.5"
  }
}
